<?xml version="1.0"?>
<Container version="2">
  <Name>external-ip-to-discord</Name>
  <Repository>niyuni/external-ip-notify</Repository>
  <Registry/>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/NiyuniCidron/_IP</Project>
  <Overview/>
  <Category>Other: Network:Other</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://github.com/NiyuniCidron/_IP/blob/main/_IP.png?raw=true</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1742446702</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Discord Webhooks" Target="DISCORD_WEBHOOKS" Default="" Mode="" Description="Provide Discord webhook to send notifications when external IP changes.&#13;&#10;&#13;&#10;You can provide more than one webhook separated by commas to notify more than one channel." Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="Check Interval" Target="CHECK_INTERVAL" Default="1" Mode="" Description="The number portion of the interval between external IP checks." Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="Check Interval Unit" Target="CHECK_INTERVAL_UNIT" Default="hours" Mode="" Description="The unit of time for the interval between external IP checks.&#13;&#10;&#13;&#10;Valid optoins: seconds, minutes, hours, or days" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="Data" Target="/app/data" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/external-ip-to-discord/data/</Config>
  <TailscaleStateDir/>
